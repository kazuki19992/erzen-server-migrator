{
  "name": "modpack-migrator",
  "version": "0.1.0",
  "private": true,
  "description": "Minecraft modpack migration helper (options.txt + journeymap)",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && vite build",
    "start": "npm run build && electron .",
    "dev:renderer": "vite",
    "dev:electron": "VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "dev:main": "tsc -p tsconfig.json -w",
    "dev": "concurrently -k \"vite\" \"tsc -p tsconfig.json -w\" \"VITE_DEV_SERVER_URL=http://localhost:5173 electron .\""
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
